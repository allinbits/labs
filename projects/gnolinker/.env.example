# Example environment file for gnolinker
# Copy this to .env, .dev.env, .stg.env, or .prod.env and fill in your values

# Discord Configuration
GNOLINKER__DISCORD_TOKEN="your-discord-bot-token"
GNOLINKER__DISCORD_GUILD_ID="your-discord-server-id"
GNOLINKER__DISCORD_ADMIN_ROLE_ID="admin-role-id"
GNOLINKER__DISCORD_VERIFIED_ROLE_ID="verified-role-id"

# Gno Configuration
GNOLINKER__SIGNING_KEY="hex-encoded-signing-key"
GNOLINKER__GNOLAND_RPC_ENDPOINT="https://rpc.gno.land:443"
GNOLINKER__BASE_URL="https://gno.land"

# Contract Paths (optional - defaults provided)
GNOLINKER__USER_CONTRACT="gno.land/r/linker000/discord/user/v0"
GNOLINKER__ROLE_CONTRACT="gno.land/r/linker000/discord/role/v0"